    "cross-env": "^5.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4"
