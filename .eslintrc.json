{
    "extends": "airbnb-base",
    "plugins": [
        "html"
    ],
    "rules": {
        "no-multiple-empty-lines": [2, {"max": 1}],
        "linebreak-style": 0,
        "indent": [2, 4],
        "import/no-unresolved": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-restricted-globals": 0,
        "max-len": 0,
        "no-loop-func": 0,
        "no-plusplus": 0
    }
}